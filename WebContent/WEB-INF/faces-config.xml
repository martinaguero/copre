<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">

<faces-config>
	<managed-bean>
		<managed-bean-name>participantBean</managed-bean-name>
		<managed-bean-class>
			ar.com.trimatec.copre.gui.ParticipantBean
		</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
		<managed-property>
			<property-name>participantService</property-name>
			<value>#{participantService}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>catalogBean</managed-bean-name>
		<managed-bean-class>
			ar.com.trimatec.copre.gui.CatalogBean
		</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
		<managed-property>
			<property-name>catalogService</property-name>
			<value>#{catalogService}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>requirementBean</managed-bean-name>
		<managed-bean-class>
			ar.com.trimatec.copre.gui.RequirementBean
		</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
		<managed-property>
			<property-name>catalogService</property-name>
			<value>#{catalogService}</value>
		</managed-property>
		<managed-property>
			<property-name>participantService</property-name>
			<value>#{participantService}</value>
		</managed-property>
		<managed-property>
			<property-name>requirementService</property-name>
			<value>#{requirementService}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>bidBean</managed-bean-name>
		<managed-bean-class>
			ar.com.trimatec.copre.gui.BidBean
		</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
		<managed-property>
			<property-name>requirementService</property-name>
			<value>#{requirementService}</value>
		</managed-property>
		<managed-property>
			<property-name>bidService</property-name>
			<value>#{bidService}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>bidAdminBean</managed-bean-name>
		<managed-bean-class>
			ar.com.trimatec.copre.gui.BidAdminBean
		</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
		<managed-property>
			<property-name>requirementService</property-name>
			<value>#{requirementService}</value>
		</managed-property>
		<managed-property>
			<property-name>bidService</property-name>
			<value>#{bidService}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>errorBean</managed-bean-name>
		<managed-bean-class>
			ar.com.trimatec.copre.gui.ErrorBean
		</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<from-view-id>/pages/newparticipant.jsp</from-view-id>
		<navigation-case>
			<from-outcome>addnewparticipant</from-outcome>
			<to-view-id>/pages/newparticipant-added.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/newproduct.jsp</from-view-id>
		<navigation-case>
			<from-outcome>addnewproduct</from-outcome>
			<to-view-id>/pages/newproduct-added.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/newrequirement-category.jsp</from-view-id>
		<navigation-case>
			<from-outcome>selectcategory</from-outcome>
			<to-view-id>/pages/newrequirement-product.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/newrequirement-product.jsp</from-view-id>
		<navigation-case>
			<from-outcome>selectproduct</from-outcome>
			<to-view-id>/pages/newrequirement-product-item.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/newrequirement-product-item.jsp</from-view-id>
		<navigation-case>
			<from-outcome>newitem</from-outcome>
			<to-view-id>/pages/newrequirement-category.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/newrequirement-product-item.jsp</from-view-id>
		<navigation-case>
			<from-outcome>displayrequirement</from-outcome>
			<to-view-id>/pages/newrequirement.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/newrequirement.jsp</from-view-id>
		<navigation-case>
			<from-outcome>saverequirement</from-outcome>
			<to-view-id>/pages/newrequirement-added.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/newrequirement-added.jsp</from-view-id>
		<navigation-case>
			<from-outcome>newrequirement</from-outcome>
			<to-view-id>/pages/newrequirement-category.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/newbid-listrequirements.jsp</from-view-id>
		<navigation-case>
			<from-outcome>bid</from-outcome>
			<to-view-id>/pages/newbid.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/newbid.jsp</from-view-id>
		<navigation-case>
			<from-outcome>savebid</from-outcome>
			<to-view-id>/pages/newbid-added.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/newbid-added.jsp</from-view-id>
		<navigation-case>
			<from-outcome>listrequirements</from-outcome>
			<to-view-id>/pages/newbid-listrequirements.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/bidadmin-listbids.jsp</from-view-id>
		<navigation-case>
			<from-outcome>acceptbid</from-outcome>
			<to-view-id>/pages/bidadmin-confirmbid.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/bidadmin-confirmbid.jsp</from-view-id>
		<navigation-case>
			<from-outcome>confirmbid</from-outcome>
			<to-view-id>/pages/bidadmin-confirmbid-added.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/bidadmin-confirmbid-added.jsp</from-view-id>
		<navigation-case>
			<from-outcome>listbids</from-outcome>
			<to-view-id>/pages/bidadmin-listbids.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/requirement-history.jsp</from-view-id>
		<navigation-case>
			<from-outcome>displayhistory</from-outcome>
			<to-view-id>/pages/requirement-history-listhistory.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/requirement-history-listhistory.jsp</from-view-id>
		<navigation-case>
			<from-outcome>newhistory</from-outcome>
			<to-view-id>/pages/requirement-history.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/bidadmin-listbids.jsp</from-view-id>
		<navigation-case>
			<from-outcome>refresh</from-outcome>
			<to-view-id>/pages/bidadmin-listbids.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/bidadmin.jsp</from-view-id>
		<navigation-case>
			<from-outcome>load</from-outcome>
			<to-view-id>/pages/bidadmin-listbids.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/newbid-listrequirements.jsp</from-view-id>
		<navigation-case>
			<from-outcome>refresh</from-outcome>
			<to-view-id>/pages/newbid-listrequirements.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<application>
		<variable-resolver>
			org.springframework.web.jsf.DelegatingVariableResolver
		</variable-resolver>
	</application>
</faces-config>